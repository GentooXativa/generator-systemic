{
  "name": "svc-example",
  "version": "1.0.3",
  "description": "An example microservice using systemic, confabulous and prepper",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "mocha test",
    "lint": "eslint .",
    "qa": "npm run lint && npm run test",
    "docker": "bash -c '(docker network inspect local 2>&1 > /dev/null || docker network create local) && docker-compose --file docker/docker-compose-local.yml pull && docker-compose --file docker/docker-compose-local.yml up -d --force-recreate'",
    "precommit": "npm run qa"
  },
  "keywords": [
    "microservice",
    "systemic",
    "confabulous",
    "prepper",
    "example"
  ],
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.0.1",
    "eslint-config-imperative-es6": "^1.0.0",
    "eslint-plugin-mocha": "^4.0.0",
    "husky": "^0.11.4",
    "make-manifest": "^0.1.2",
    "mocha": "^2.5.3",
    "request": "^2.72.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "chalk": "^1.1.3",
    "confabulous": "^1.0.0",
    "debug": "^2.2.0",
    "hogan.js": "^3.0.2",
    "lodash.get": "^4.3.0",
    "lodash.merge": "^4.4.0",
    "lodash.omit": "^4.3.0",
    "lodash.pick": "^4.2.1",
    "lodash.pluck": "^3.1.2",
    "on-headers": "^1.0.1",
    "optimist": "^0.6.1",
    "optional": "^0.1.3",
    "prepper": "^0.1.0",
    "systemic": "^0.1.3",
    "systemic-domain-runner": "^0.1.4",
    "systemic-express": "^1.0.1",
    "systemic-mongodb": "^1.0.3",
    "systemic-pg": "^1.0.6",
    "systemic-redis": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/svc-example.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/svc-example/issues"
  },
  "homepage": "https://github.com/guidesmiths/svc-example#readme"
}
